{
    "application.shellEnvironmentResolutionTimeout": 120,
    "block.comment": "Three liveshare configs: liveshare.guestApprovalRequired, liveshare.anonymousGuestApproval, and liveshare.shareExternalFiles, have been added to your VS Code config by the Block CPE team. They are a mandatory security requirement which must be met (CPENG-706). Please leave them as-is.",
    "diffEditor.codeLens": true,
    "editor.accessibilitySupport": "off",
    "editor.cursorStyle": "line",
    "editor.fontFamily": "Source Code Pro",
    "editor.fontSize": 16,
    "editor.inlineSuggest.enabled": true,
    "editor.insertSpaces": false,
    "editor.letterSpacing": 2,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": false,
        "scminput": false
    },
    "kotlin.debugAdapter.enabled": false,
    "kotlin.languageServer.enabled": false,
    "liveshare.anonymousGuestApproval": "reject",
    "liveshare.guestApprovalRequired": true,
    "liveshare.shareExternalFiles": false,
    "security.workspace.trust.untrustedFiles": "open",
    "vim.useSystemClipboard": true,
    "cursor.cpp.disabledLanguages": [
        "markdown",
        "plaintext",
        "scminput"
    ],
    "redhat.telemetry.enabled": true,
    "terminal.integrated.defaultProfile.osx": "zsh"
}